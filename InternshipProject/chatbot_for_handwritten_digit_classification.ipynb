        "# --- 2. Load and Prepare Data ---\n",
        "def load_data(file_path):\n",
        "    with open(file_path, 'r', encoding='utf-8') as f:\n",
        "        data = json.load(f)\n",
        "    texts = []\n",
        "    labels = []\n",
        "    for item in data:\n",
        "        for text in item['input_text']:\n",
        "            texts.append(text)\n",
        "            labels.append(item['tag'])\n",
        "    return texts, labels\n",
        "\n",
        "print(\"Loading data...\")\n",
        "all_texts, all_labels = load_data(DATA_FILE)\n",
        "\n",
        "# Encode labels\n",
        "label_encoder = LabelEncoder()\n",
        "encoded_labels = label_encoder.fit_transform(all_labels)\n",
        "num_labels = len(label_encoder.classes_)\n",
        "print(f\"Detected {num_labels} unique intents: {label_encoder.classes_}\")\n",
        "\n",
        "# Initialize tokenizer\n",
        "tokenizer = DistilBertTokenizerFast.from_pretrained(MODEL_NAME)\n",
        "\n",
        "# Tokenize all texts\n",
        "print(\"Tokenizing data...\")\n",
        "# Crucial: Use consistent max_length for both training and inference\n",
        "tokenized_inputs = tokenizer(\n",
        "    all_texts,\n",
        "    truncation=True,\n",
        "    padding='max_length', # Pad to MAX_SEQ_LENGTH\n",
        "    max_length=MAX_SEQ_LENGTH\n",
        ")\n",
        "\n",
        "# Create Dataset compatible with Hugging Face Trainer\n",
        "class ChatbotDataset(torch.utils.data.Dataset):\n",
        "    def __init__(self, encodings, labels):\n",
        "        self.encodings = encodings\n",
        "        self.labels = labels\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        item = {key: torch.tensor(val[idx]) for key, val in self.encodings.items()}\n",
        "        item['labels'] = torch.tensor(self.labels[idx])\n",
        "        return item\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.labels)\n",
        "\n",
        "# For simplicity in this example, we'll use all data for training.\n",
        "# In a real scenario, you'd split into train/validation sets.\n",
        "full_dataset = ChatbotDataset(tokenized_inputs, encoded_labels)"
